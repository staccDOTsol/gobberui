#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/volume_lon1_01/gobogb/components/ts-client/node_modules/.pnpm/ts-mocha@10.0.0_mocha@10.2.0/node_modules/ts-mocha/bin/node_modules:/mnt/volume_lon1_01/gobogb/components/ts-client/node_modules/.pnpm/ts-mocha@10.0.0_mocha@10.2.0/node_modules/ts-mocha/node_modules:/mnt/volume_lon1_01/gobogb/components/ts-client/node_modules/.pnpm/ts-mocha@10.0.0_mocha@10.2.0/node_modules:/mnt/volume_lon1_01/gobogb/components/ts-client/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/volume_lon1_01/gobogb/components/ts-client/node_modules/.pnpm/ts-mocha@10.0.0_mocha@10.2.0/node_modules/ts-mocha/bin/node_modules:/mnt/volume_lon1_01/gobogb/components/ts-client/node_modules/.pnpm/ts-mocha@10.0.0_mocha@10.2.0/node_modules/ts-mocha/node_modules:/mnt/volume_lon1_01/gobogb/components/ts-client/node_modules/.pnpm/ts-mocha@10.0.0_mocha@10.2.0/node_modules:/mnt/volume_lon1_01/gobogb/components/ts-client/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-mocha/bin/ts-mocha" "$@"
else
  exec node  "$basedir/../ts-mocha/bin/ts-mocha" "$@"
fi
