#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/volume_lon1_01/gobogb/components/ts-client/node_modules/.pnpm/ts-jest@28.0.8_@babel+core@7.21.4_@jest+types@28.1.3_babel-jest@28.1.3_@babel+core@7.21.4__je_tny7dk3wzh7lgo7nzgyay3wqxi/node_modules/ts-jest/node_modules:/mnt/volume_lon1_01/gobogb/components/ts-client/node_modules/.pnpm/ts-jest@28.0.8_@babel+core@7.21.4_@jest+types@28.1.3_babel-jest@28.1.3_@babel+core@7.21.4__je_tny7dk3wzh7lgo7nzgyay3wqxi/node_modules:/mnt/volume_lon1_01/gobogb/components/ts-client/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/volume_lon1_01/gobogb/components/ts-client/node_modules/.pnpm/ts-jest@28.0.8_@babel+core@7.21.4_@jest+types@28.1.3_babel-jest@28.1.3_@babel+core@7.21.4__je_tny7dk3wzh7lgo7nzgyay3wqxi/node_modules/ts-jest/node_modules:/mnt/volume_lon1_01/gobogb/components/ts-client/node_modules/.pnpm/ts-jest@28.0.8_@babel+core@7.21.4_@jest+types@28.1.3_babel-jest@28.1.3_@babel+core@7.21.4__je_tny7dk3wzh7lgo7nzgyay3wqxi/node_modules:/mnt/volume_lon1_01/gobogb/components/ts-client/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../ts-jest/cli.js" "$@"
fi
