#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/volume_lon1_01/gobogb/components/ts-client/node_modules/.pnpm/@solana+errors@2.0.0-preview.4_typescript@5.5.4/node_modules/@solana/errors/bin/node_modules:/mnt/volume_lon1_01/gobogb/components/ts-client/node_modules/.pnpm/@solana+errors@2.0.0-preview.4_typescript@5.5.4/node_modules/@solana/errors/node_modules:/mnt/volume_lon1_01/gobogb/components/ts-client/node_modules/.pnpm/@solana+errors@2.0.0-preview.4_typescript@5.5.4/node_modules/@solana/node_modules:/mnt/volume_lon1_01/gobogb/components/ts-client/node_modules/.pnpm/@solana+errors@2.0.0-preview.4_typescript@5.5.4/node_modules:/mnt/volume_lon1_01/gobogb/components/ts-client/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/volume_lon1_01/gobogb/components/ts-client/node_modules/.pnpm/@solana+errors@2.0.0-preview.4_typescript@5.5.4/node_modules/@solana/errors/bin/node_modules:/mnt/volume_lon1_01/gobogb/components/ts-client/node_modules/.pnpm/@solana+errors@2.0.0-preview.4_typescript@5.5.4/node_modules/@solana/errors/node_modules:/mnt/volume_lon1_01/gobogb/components/ts-client/node_modules/.pnpm/@solana+errors@2.0.0-preview.4_typescript@5.5.4/node_modules/@solana/node_modules:/mnt/volume_lon1_01/gobogb/components/ts-client/node_modules/.pnpm/@solana+errors@2.0.0-preview.4_typescript@5.5.4/node_modules:/mnt/volume_lon1_01/gobogb/components/ts-client/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@solana+errors@2.0.0-preview.4_typescript@5.5.4/node_modules/@solana/errors/bin/cli.mjs" "$@"
else
  exec node  "$basedir/../../../../../../@solana+errors@2.0.0-preview.4_typescript@5.5.4/node_modules/@solana/errors/bin/cli.mjs" "$@"
fi
